# Example .env for Claude/Anthropic and Google Vision integration
# Copy this file to .env and set your API keys

# Claude/Anthropic Vision (optional)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Cloud Vision API (optional)
# Set this to the path of your Google Cloud service account JSON key
# Alternatively, use GOOGLE_APPLICATION_CREDENTIALS env var
GOOGLE_CLOUD_CREDENTIALS=/path/to/google-service-account.json

# Enable/disable vision models (optional, default: auto-detect)
ENABLE_CLAUDE_VISION=true
ENABLE_GOOGLE_VISION=true

